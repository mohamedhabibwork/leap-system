{
  "name": "@leap-lms/backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "node -e \"const fs = require('fs'); const path = require('path'); const dir = path.join(__dirname, 'dist', 'grpc', 'proto'); if (!fs.existsSync(dir)) { fs.mkdirSync(dir, { recursive: true }); }\"",
    "build": "nest build",
    "postbuild": "node -e \"const fs = require('fs'); const path = require('path'); const srcDir = path.join(__dirname, 'src', 'grpc', 'proto'); const destDir = path.join(__dirname, 'dist', 'grpc', 'proto'); if (fs.existsSync(srcDir)) { if (!fs.existsSync(destDir)) { fs.mkdirSync(destDir, { recursive: true }); } fs.readdirSync(srcDir).forEach(file => { if (file.endsWith('.proto')) { fs.copyFileSync(path.join(srcDir, file), path.join(destDir, file)); } }); }\"",
    "postinstall": "npm rebuild bcrypt --build-from-source || echo 'bcrypt rebuild skipped'",
    "rebuild:bcrypt": "npm rebuild bcrypt --build-from-source",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:faker": "jest --config ./test/jest-e2e.json --testPathPatterns=\"(get-endpoints|data-validation)\"",
    "test:faker:e2e": "jest --config ./test/jest-e2e.json --testPathPatterns=\"(get-endpoints|data-validation)\"",
    "test:faker:integration": "jest --testPathPatterns=\"integration\"",
    "test:faker:watch": "jest --config ./test/jest-e2e.json --testPathPatterns=\"(get-endpoints|data-validation)\" --watch",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeders/index.ts",
    "seed:bun": "bun --bun src/database/seeders/index.ts",
    "seed:reset": "ts-node -r tsconfig-paths/register src/database/seeders/index.ts --reset",
    "seed:reset:bun": "bun --bun src/database/seeders/index.ts --reset",
    "seed:keycloak:roles": "ts-node -r tsconfig-paths/register src/database/seeders/index.ts --keycloak-roles",
    "seed:keycloak:roles:bun": "bun --bun src/database/seeders/index.ts --keycloak-roles",
    "seed:keycloak:users": "ts-node -r tsconfig-paths/register src/database/seeders/index.ts --keycloak-users",
    "seed:keycloak:users:bun": "bun --bun src/database/seeders/index.ts --keycloak-users",
    "seed:keycloak:all": "ts-node -r tsconfig-paths/register src/database/seeders/index.ts --keycloak-all",
    "seed:keycloak:all:bun": "bun --bun src/database/seeders/index.ts --keycloak-all"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@aws-sdk/client-s3": "^3.965.0",
    "@aws-sdk/s3-request-presigner": "^3.965.0",
    "@grpc/grpc-js": "^1.12.4",
    "@grpc/proto-loader": "^0.7.14",
    "@grpc/reflection": "^1.0.4",
    "@keycloak/keycloak-admin-client": "^26.5.0",
    "@nestjs/apollo": "^12.2.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.15",
    "@nestjs/graphql": "^12.2.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.15",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.21",
    "@nestjs/platform-socket.io": "^10.4.21",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/websockets": "^10.4.21",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@types/amqplib": "^0.10.5",
    "@types/minio": "^7.1.0",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "amqplib": "^0.10.5",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "drizzle-orm": "^0.36.4",
    "express-session": "^1.18.1",
    "firebase-admin": "^13.0.2",
    "graphql": "^16.12.0",
    "ioredis": "^5.4.2",
    "jwks-rsa": "^3.2.0",
    "kafkajs": "^2.2.4",
    "keycloak-connect": "^25.0.6",
    "minio": "^8.0.6",
    "multer": "^1.4.5-lts.1",
    "nest-keycloak-connect": "^1.10.0",
    "nodemailer": "^7.0.11",
    "oidc-provider": "^9.6.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-openidconnect": "^0.1.1",
    "pdfkit": "^0.15.1",
    "postgres": "^3.4.5",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@nestjs/cli": "^10.4.7",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.15",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^30.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/pdfkit": "^0.13.5",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.2",
    "jest": "^30.0.0",
    "pg": "^8.16.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  }
}
